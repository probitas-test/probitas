{
  "name": "@lambdalisue/probitas",
  "exports": {
    ".": "./mod.ts",
    "./builder": "./src/builder/mod.ts",
    "./cli": "./cli/mod.ts",
    "./reporter": "./src/reporter/mod.ts",
    "./runner": "./src/runner/mod.ts"
  },
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "assets/**/*",
      "**/*.ts"
    ],
    "exclude": [
      "**/*_test.ts",
      "**/*_bench.ts",
      "tests/**/*",
      "example/**/*"
    ]
  },
  "exclude": [
    ".coverage/**"
  ],
  "tasks": {
    "cli": "deno run -A ./cli/mod.ts",
    "check": "deno check ./**/*.ts",
    "test": "deno test -A --parallel --shuffle",
    "test:coverage": "deno task test --coverage=.coverage",
    "coverage": "deno coverage .coverage --include='/(src|cli|mod\\.ts)/'"
  },
  "imports": {
    "@core/errorutil": "jsr:@core/errorutil@^1.2.1",
    "@core/unknownutil": "jsr:@core/unknownutil@^4.3.0",
    "@cspotcode/outdent": "jsr:@cspotcode/outdent@^0.8.0",
    "@lambdalisue/sandbox": "jsr:@lambdalisue/sandbox@^3.0.1",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/async": "jsr:@std/async@^1.0.15",
    "@std/cli": "jsr:@std/cli@^1.0.24",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@std/expect": "jsr:@std/expect@^1.0.17",
    "@std/fmt": "jsr:@std/fmt@^1.0.8",
    "@std/fs": "jsr:@std/fs@^1.0.20",
    "@std/http": "jsr:@std/http@^1.0.10",
    "@std/jsonc": "jsr:@std/jsonc@^1.0.2",
    "@std/path": "jsr:@std/path@^1.1.3",
    "@std/semver": "jsr:@std/semver@^1.0.7",
    "@std/streams": "jsr:@std/streams@^1.0.14",
    "@std/testing": "jsr:@std/testing@^1.0.16",
    "probitas": "./mod.ts",
    "probitas/builder": "./src/builder/mod.ts",
    "probitas/cli": "./cli/mod.ts",
    "probitas/reporter": "./src/reporter/mod.ts",
    "probitas/runner": "./src/runner/mod.ts"
  }
}
