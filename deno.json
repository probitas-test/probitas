{
  "name": "@probitas/probitas",
  "version": "0.18.1",
  "exports": {
    ".": "./src/mod.ts",
    "./cli": "./src/cli.ts",
    "./client": "./src/client.ts",
    "./client/connectrpc": "./src/client/connectrpc.ts",
    "./client/deno-kv": "./src/client/deno_kv.ts",
    "./client/graphql": "./src/client/graphql.ts",
    "./client/grpc": "./src/client/grpc.ts",
    "./client/http": "./src/client/http.ts",
    "./client/http/oidc": "./src/client/http/oidc.ts",
    "./client/mongodb": "./src/client/mongodb.ts",
    "./client/rabbitmq": "./src/client/rabbitmq.ts",
    "./client/redis": "./src/client/redis.ts",
    "./client/sql": "./src/client/sql.ts",
    "./client/sql/duckdb": "./src/client/sql/duckdb.ts",
    "./client/sql/mysql": "./src/client/sql/mysql.ts",
    "./client/sql/postgres": "./src/client/sql/postgres.ts",
    "./client/sql/sqlite": "./src/client/sql/sqlite.ts",
    "./client/sqs": "./src/client/sqs.ts",
    "./expect": "./src/expect.ts"
  },
  "publish": {
    "exclude": [
      "**/*_test.ts",
      "**/*_bench.ts"
    ],
    "include": [
      "LICENSE",
      "README.md",
      "assets/**/*",
      "deno.json",
      "deno.lock",
      "src/**/*.ts"
    ]
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-import-prefix"
      ]
    }
  },
  "exclude": [
    ".coverage/**",
    ".claude/**",
    ".worktrees/**",
    "dist/**"
  ],
  "tasks": {
    "probitas": "deno run -A --unstable-kv --lock --frozen ./src/cli.ts",
    "check": "deno check --doc && deno check --doc-only *.md .claude/**/*.md",
    "test": "deno test -A --parallel --shuffle",
    "test:coverage": "deno task test --coverage=.coverage",
    "coverage": "deno coverage .coverage --include='/src/'",
    "verify": "deno fmt && deno lint && deno task check && deno task test",
    "release": "deno run -A .scripts/release.ts",
    "compile": "deno run -A scripts/build_subprocess_templates.ts && deno compile -A --unstable-kv -o dist/probitas ./src/cli.ts"
  },
  "imports": {
    "@core/streamutil": "jsr:@core/streamutil@^1.0.0",
    "@deno/graph": "jsr:@deno/graph@^0.105.0",
    "@core/errorutil": "jsr:@core/errorutil@^1.2.1",
    "@core/unknownutil": "jsr:@core/unknownutil@^4.3.0",
    "@std/async": "jsr:@std/async@^1.0.16",
    "@std/async/deadline": "jsr:@std/async@^1.0.16/deadline",
    "@std/async/delay": "jsr:@std/async@^1.0.16/delay",
    "@cspotcode/outdent": "jsr:@cspotcode/outdent@^0.8.0",
    "@faker-js/faker": "npm:@faker-js/faker@^10.2.0",
    "@lambdalisue/sandbox": "jsr:@lambdalisue/sandbox@^3.0.1",
    "@logtape/logtape": "jsr:@logtape/logtape@^1.3.5",
    "@logtape/pretty": "jsr:@logtape/pretty@^1.3.5",
    "@probitas/builder": "jsr:@probitas/builder@^0.5.1",
    "@probitas/client-connectrpc": "jsr:@probitas/client-connectrpc@^0.7.0",
    "@probitas/client-deno-kv": "jsr:@probitas/client-deno-kv@^0.5.0",
    "@probitas/client-graphql": "jsr:@probitas/client-graphql@^0.6.0",
    "@probitas/client-grpc": "jsr:@probitas/client-grpc@^0.6.0",
    "@probitas/client-http": "jsr:@probitas/client-http@^0.7.2",
    "@probitas/client-mongodb": "jsr:@probitas/client-mongodb@^0.6.0",
    "@probitas/client-rabbitmq": "jsr:@probitas/client-rabbitmq@^0.5.0",
    "@probitas/client-redis": "jsr:@probitas/client-redis@^0.5.0",
    "@probitas/client-sql": "jsr:@probitas/client-sql@^0.5.0",
    "@probitas/client-sql-duckdb": "jsr:@probitas/client-sql-duckdb@^0.5.0",
    "@probitas/client-sql-mysql": "jsr:@probitas/client-sql-mysql@^0.5.0",
    "@probitas/client-sql-postgres": "jsr:@probitas/client-sql-postgres@^0.5.0",
    "@probitas/client-sql-sqlite": "jsr:@probitas/client-sql-sqlite@^0.5.0",
    "@probitas/client-sqs": "jsr:@probitas/client-sqs@^0.5.0",
    "@probitas/core": "jsr:@probitas/core@^0.3.1",
    "@probitas/discover": "jsr:@probitas/discover@^0.4.0",
    "@probitas/expect": "jsr:@probitas/expect@^0.4.0",
    "@probitas/reporter": "jsr:@probitas/reporter@^0.7.1",
    "@probitas/runner": "jsr:@probitas/runner@^0.5.1",
    "@std/assert": "jsr:@std/assert@^1.0.16",
    "@std/cbor": "jsr:@std/cbor@^0.1.9",
    "@std/cli": "jsr:@std/cli@^1.0.25",
    "@std/collections": "jsr:@std/collections@^1.1.3",
    "@std/collections/chunk": "jsr:@std/collections@^1.1.3/chunk",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.6",
    "@std/fs": "jsr:@std/fs@^1.0.21",
    "@std/json": "jsr:@std/json@^1.0.2",
    "@std/jsonc": "jsr:@std/jsonc@^1.0.2",
    "@std/path": "jsr:@std/path@^1.1.4",
    "@std/streams": "jsr:@std/streams@^1.0.16",
    "@std/testing/bdd": "jsr:@std/testing@^1.0.16/bdd",
    "@std/testing/mock": "jsr:@std/testing@^1.0.16/mock",
    "@std/testing/time": "jsr:@std/testing@^1.0.16/time",
    "jsr:@probitas/probitas@^0": "./src/mod.ts"
  }
}
